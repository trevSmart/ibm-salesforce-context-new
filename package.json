{
	"name": "test_research4",
	"version": "0.3.37",
	"description": "Test Research 4",
	"main": "index.js",
	"type": "module",
	"browser": {
		"child_process": false
	},
	"bin": {
		"test_research4": "bin/cli.js"
	},
	"scripts": {
		"lint:fix": "biome check . --write --unsafe",
		"update-readme": "node dev/updateReadme.js",
		"test:mcpInspector": "sh dev/startMcpInspector.sh",
		"test": "vitest --silent --run",
		"test:cov": "c8 -o coverage --reporter=text --reporter=html vitest run",
		"test:watch": "vitest --watch",
		"test:cleanup": "pkill -f 'node.*index.js' || true",
		"test:clean": "npm run test:cleanup && npm run test",
		"start": "node index.js --transport http",
		"publish-package": "sh dev/publish-package.sh",
		"publish-package-skip-tests": "sh dev/publish-package.sh --skip-tests",
		"publish-package-skip-obfuscation": "sh dev/publish-package.sh --skip-obfuscation",
		"publish-package-skip-tests-and-obfuscation": "sh dev/publish-package.sh --skip-tests --skip-obfuscation"
	},
	"keywords": [
		"test",
		"research"
	],
	"author": "Trevor Smart",
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.20.0",
		"chokidar": "^4.0.3",
		"conf": "^15.0.1",
		"express": "^5.1.0",
		"fs-extra": "^11.3.1",
		"node-schedule": "^2.1.1",
		"path-to-regexp": "^8.3.0",
		"playwright": "^1.55.0",
		"semver": "^7.5.4",
		"zod": "^3.24.2"
	},
	"engines": {
		"node": ">=22.7.0 <23"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@modelcontextprotocol/inspector": "^0.17.0",
		"@salesforce/cli": "^2.108.6",
		"@vitest/coverage-v8": "^3.2.4",
		"c8": "^10.1.3",
		"dotenv": "^17.2.2",
		"globals": "^16.3.0",
		"javascript-obfuscator": "^4.1.0",
		"microscope-mcp-client": "^0.1.32",
		"node-fetch": "^3.3.2",
		"vitest": "^3.2.4"
	},
	"overrides": {
		"brace-expansion": "^3.0.1",
		"path-to-regexp": "^8.3.0",
		"tar-fs": "^3.1.1"
	}
}
