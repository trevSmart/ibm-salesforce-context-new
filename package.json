{
	"name": "test_research4",
	"version": "0.3.29",
	"description": "Test Research 4",
	"main": "index.js",
	"type": "module",
	"browser": {
		"child_process": false
	},
	"bin": {
		"test_research4": "bin/cli.js"
	},
	"scripts": {
		"lint:fix": "biome check . --write --unsafe",
		"update-readme": "node dev/updateReadme.js",
		"test:mcpInspector": "sh dev/startMcpInspector.sh",
		"test": "vitest --silent --run",
		"test:cov": "c8 -o coverage --reporter=text --reporter=html vitest run",
		"test:watch": "vitest --watch",
		"start": "node index.js --transport http",
		"publish-package": "sh dev/publish-package.sh",
		"publish-package-skip-tests": "sh dev/publish-package.sh --skip-tests"
	},
	"keywords": [
		"test",
		"research"
	],
	"author": "Trevor Smart",
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.17.4",
		"chokidar": "^4.0.3",
		"conf": "^14.0.0",
		"dotenv": "^17.2.2",
		"env-paths": "^3.0.0",
		"express": "^5.1.0",
		"fs-extra": "^11.3.1",
		"node-fetch": "^3.3.2",
		"node-schedule": "^2.1.1",
		"os": "^0.1.2",
		"playwright": "^1.55.0",
		"semver": "^7.5.4",
		"zod": "^3.24.2"
	},
	"engines": {
		"node": ">=22.7.0 <23"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.3",
		"@modelcontextprotocol/inspector": "^0.16.6",
		"@vitest/coverage-v8": "^3.2.4",
		"c8": "^10.1.3",
		"globals": "^16.3.0",
		"javascript-obfuscator": "^4.1.0",
		"vitest": "^3.2.4"
	}
}
